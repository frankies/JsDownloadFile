<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  
  		<servlet-name>x</servlet-name>
  		<servlet-class>org.test.servlet.FileDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>x</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  
  		<servlet-name>c</servlet-name>
  		<servlet-class>org.test.servlet.CookiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>c</servlet-name>
  	<url-pattern>*.ck</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
